REACT_APP_BACKEND_URL=http://localhost:8000
# Base URL for contract/pdf assets (local dev: /assets/contracts)
REACT_APP_ASSET_BASE_URL=/assets/contracts
# Base URL for images used by card templates (local dev: /images)
REACT_APP_IMAGE_BASE_URL=/images
REACT_APP_CONFIG_VERSION=2026-LOCAL
# Eden Frontend Environment Configuration
# Copy this file to .env and fill in the values

# REQUIRED: Backend API URL (must be accessible from frontend)
REACT_APP_BACKEND_URL=http://localhost:8000

# OPTIONAL: Offline queue feature flag
REACT_APP_ENABLE_OFFLINE_QUEUE=true

# OPTIONAL: Sentry error tracking (production only)
# Get your DSN from https://sentry.io
REACT_APP_SENTRY_DSN=
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=0.1.0
