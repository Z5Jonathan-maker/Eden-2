{
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://eden-2.onrender.com/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://eden-2.onrender.com/health"
    },
    {
      "source": "/ws/:path*",
      "destination": "https://eden-2.onrender.com/ws/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
