{
  "app": "Eden Claims Platform",
  "branch": "cursor/app-functionality-issues-e802",
  "config_version": "2026-02-07-pricing-badge-fix-v1",
  "beacon": {
    "endpoint": "/api/ops/beacon",
    "manifest_endpoint": "/api/ops/manifest",
    "notes": "Public endpoints intended for polling by deployment automation."
  },
  "required_env": {
    "backend": [
      "EDEN_CONFIG_VERSION",
      "TWILIO_ACCOUNT_SID",
      "TWILIO_AUTH_TOKEN",
      "TWILIO_CONVERSATIONS_SERVICE_SID",
      "TWILIO_API_KEY",
      "TWILIO_API_SECRET",
      "TWILIO_FROM_NUMBER",
      "TWILIO_VOICE_FROM_NUMBER",
      "PUBLIC_BASE_URL",
      "SMS_DRY_RUN",
      "SMS_WEBHOOK_SECRET"
    ],
    "frontend": [
      "REACT_APP_BACKEND_URL",
      "REACT_APP_CONFIG_VERSION"
    ]
  },
  "webhooks": {
    "sms_inbound_and_status": "{PUBLIC_BASE_URL}/api/sms/twilio/webhook",
    "voice_inbound": "{PUBLIC_BASE_URL}/api/twilio/voice/inbound",
    "voice_status": "{PUBLIC_BASE_URL}/api/twilio/voice/status"
  },
  "ui_routes": [
    "/comms/chat",
    "/comms/claim/:claimId",
    "/comms/dialer"
  ],
  "notes": [
    "Set secrets in your deployment platform. Do not commit secrets to git.",
    "If SMS_WEBHOOK_SECRET is set, append ?secret=... to SMS webhook URL."
  ]
}
