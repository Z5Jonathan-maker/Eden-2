{
  "version": 2,
  "buildCommand": "cd frontend && npm run build && cp -r build/. ../build/",
  "outputDirectory": "build",
  "framework": null,
  "installCommand": "cd frontend && npm install",
  "devCommand": "cd frontend && npm run dev",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://eden-gsot.onrender.com/api/:path*"
    },
    {
      "source": "/health",
      "destination": "https://eden-gsot.onrender.com/health"
    },
    {
      "source": "/ws/:path*",
      "destination": "https://eden-gsot.onrender.com/ws/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
